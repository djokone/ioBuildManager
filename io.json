{
  "name": "io-build-manager",
  "alias": {
    "sites": "e:/sites",
    "factory": "@sites/myModules",
    "workflow": "@factory/workflow",
    "factoryNpm": "@factory/NPM",
    "io": "@factoryNpm/iorizon",
    "builds": "@factoryNpm/Builds"
  },
  "options": {
    "--destination|-d": "e:/sites/myModules/NPM/Builds",
    "--interactive|-it": true,
    "--name": "webpack-build",
    "--as": "webpack",
    "--include|-i": ["package.json"]
  },
  "workflow": "@workflow/imperial",
  "modules": {
    "status": {
      "url": "./bin/status"
    },
    "save": {
      "url": "./bin/save"
    },
    "change": {
      "url": "./bin/change"
    },
    "list": {
      "url": "./bin/list"
    }
  }
}